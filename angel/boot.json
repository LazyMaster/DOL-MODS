{
  "name": "DOL_Magic",
  "version": "1.1.21",
  "styleFileList": [],
  "scriptFileList": [],
  "tweeFileList": [
    "twee/angel_perk.twee",
    "twee/widget.twee"
  ],
  "imgFileList": [],
  "additionFile": [
    "addon-replace/angel_pray_for_salvation.txt",
    "addon-replace/angel_rob_purity.txt"
  ],
  "addonPlugin": [
    {
      "modName": "TweeReplacer",
      "addonName": "TweeReplacerAddon",
      "modVersion": "1.0.0",
      "params": [
        {
          "passage": "Widgets Attitudes",
          "findString": "<<loveInterestFunction>>",
          "replace": "<<loveInterestFunction>><<EroAngel_setting>>"
        },
        {
          "passage": "Transformation Widgets",
          "findString": "<<widget \"fallenTransform\">>\n\t<<if $angel gte 6>>",
          "replace": "<<widget \"fallenTransform\">><<EroAngel_setting_init>>\n\t<<if $angel gte 6 and $EroAngel_NoFallen is false>>"
        },
        {
          "passage": "Transformation Widgets",
          "findString": "\t\t<<earnFeat \"Fallen Angel\">>\n\t<<elseif $angel gte 4>>",
          "replace": "\t\t<<earnFeat \"Fallen Angel\">>\n\t<<elseif $angel gte 4 and $EroAngel_NoFallen is false>>"
        },
        {
          "passage": "Transformation Widgets",
          "findString": "\t<<if $angel gt 0>>\n\t\t<<angelTransform 99>>\n\t<</if>>",
          "replace": "\t<<if $angel gt 0 and $EroAngel_NoFallen is false>>\n\t\t<<angelTransform 99>>\n\t<</if>>"
        },
        {
          "passage": "Widgets Effects Man",
          "findString": "\t\t\t\t<<set $trauma -= $traumagain>><<set $traumagain to 0>><<set $speechforgive to 1>><<set $angelforgive to 1>>",
          "replace": "\t\t\t\t<<set $trauma -= $traumagain>><<if $EroAngel_angelforgive>><<set $pain to 0>><<set $stress to 0>><</if>><<set $traumagain to 0>><<set $speechforgive to 1>><<set $angelforgive to 1>>"
        },
        {
          "passage": "Widgets End Combat",
          "findString": "\t<<if $demonabsorb gte 1",
          "replace": "\t<<EA_Combat_End>><<if $demonabsorb gte 1"
        },
        {
          "passage": "Prison Yard Flirt",
          "findString": "<<if $demon gte 6",
          "replace": "<<if $angel gte 6>><<link [[獻身|Prison Yard Flirt Fix]]>><</link>><<angel>><</if>><<if $demon gte 6"
        },
        {
          "passage": "Widgets Orgasm",
          "findString": "<<widget \"breastfeed\">>\n\t<<set $_milk_released to random(10, 50)>>",
          "replace": "<<widget \"breastfeed\">>\n\t<<set $_milk_released to random(10, 200)>>"
        },
        {
          "passage": "Widgets Combat",
          "findString": "\t\t\t\t\t<<arousal 2000>><<ggarousal>>",
          "replaceFile": "addon-replace/angel_rob_purity.txt",
          "all": true
        },
        {
          "passage": "Widgets Temple",
          "findString": "\t\t<<lawareness>><<lstress>><<larousal>>",
          "replaceFile": "addon-replace/angel_pray_for_salvation.txt"
        },
        {
          "passage": "Widgets Masturbation Actions",
          "findString": "$awareness gte 100",
          "replace": "($awareness gte 100 or $EroAngel_Noawareness)",
          "all": true
        },
        {
          "passage": "Widgets Masturbation Actions",
          "findString": "$awareness gte 200",
          "replace": "($awareness gte 200 or $EroAngel_Noawareness)",
          "all": true
        },
        {
          "passage": "Widgets Masturbation Actions",
          "findString": "$awareness gte 400",
          "replace": "($awareness gte 400 or $EroAngel_Noawareness)",
          "all": true
        },
        {
          "passage": "Widgets Sydney",
          "findString": "<<if $promiscuity gte 55 and $sydneyromance is 1 and !$daily.sydney.crawl>>",
          "replace": "<<EroAngel_ExpelIdlers>><<if $promiscuity gte 55 and $sydneyromance is 1 and !$daily.sydney.crawl>>"
        },
        {
          "passage": "Sydney Crawl",
          "findString": "<<if [\"early\",\"late\"].includes($schoolstate)>>",
          "replace": "<<if [\"early\",\"late\"].includes($schoolstate) or $EroAngel_daily_expel_idlers>>"
        },
        {
          "passage": "Temple Confess Start",
          "findString": "Temple Confess]]>><<pass 30>><</link>>",
          "replace": "Temple Confess]]>><<pass 30>><</link>><<EA_CallCorrupt>><<EA_CallSidney>>"
        },
        {
          "passage": "Temple Confess",
          "findString": "sydney_promised_1\" 4>>",
          "replace": "sydney_promised_1\" $EA_CallSidney_On>>"
        },
        {
          "passage": "Temple Confess",
          "findString": "sydney_promised_2\" 3>>",
          "replace": "sydney_promised_2\" $EA_CallSidney_On>>"
        },
        {
          "passage": "Temple Confess",
          "findString": "sydney_romance\" 2>>",
          "replace": "sydney_romance\" $EA_CallSidney_On>>"
        },
        {
          "passage": "Temple Confess",
          "findString": "sydney_love\" 1>>",
          "replace": "sydney_love\" $EA_CallSidney_On>>>>"
        },
        {
          "passage": "Temple Confess",
          "findString": "sydney_kylar\" 0.2>>",
          "replace": "sydney_kylar\" $EA_CallSidney_On>>"
        },
        {
          "passage": "Temple Confess",
          "findString": "0.7>>",
          "replace": "$EA_CallCorrupt_On>>",
          "all": true
        },
        {
          "passage": "Temple Confess",
          "findString": "fake\" $EA_CallCorrupt_On",
          "replace": "fake\" 0.7"
        },
        {
          "passage": "Widgets Actions Generation",
          "findString": "<<mouthActions>>",
          "replace": "<<mouthActions>><<EA_mouthaction_add>>"
        },
        {
          "passage": "Widgets Effects Man",
          "findString": "<<if $mouthaction is \"ask\" and $askAction is \"askrough\"",
          "replace": "<<EA_mouth_effect_add>><<if $mouthaction is \"ask\" and $askAction is \"askrough\""
        },
        {
          "passage": "Widgets Docks",
          "findString": "<<if $skul_dock[_args[0]].state is \"known\">>",
          "replace": "<<if $skul_dock[_args[0]].state is \"known\" or $EA_Transpass_perk and $skul_dock[_args[0]].state isnot \"empty\">>"
        }
      ]
    },
    {
      "modName": "ReplacePatcher",
      "addonName": "ReplacePatcherAddon",
      "modVersion": "1.0.0",
      "params": {
        "js": [
          {
            "from": "fragment.append(dailySchoolEffects());",
            "to": "fragment.append(wikifier(\"EroAngel_dayly\"));fragment.append(dailySchoolEffects());",
            "fileName": "time.js"
          },
          {
            "from": "V.awareness >= 100",
            "to": "(V.awareness >= 100 || V.EroAngel_Noawareness)",
            "fileName": "actions.js"
          },
          {
            "from": "V.awareness >= 200",
            "to": "(V.awareness >= 200 || V.EroAngel_Noawareness)",
            "fileName": "actions.js"
          },
          {
            "from": "V.awareness >= 300",
            "to": "(V.awareness >= 300 || V.EroAngel_Noawareness)",
            "fileName": "actions.js",
            "all": true
          },
          {
            "from": "V.awareness >= 400",
            "to": "(V.awareness >= 400 || V.EroAngel_Noawareness)",
            "fileName": "actions.js"
          }
        ]
      }
    },
    {
      "modName": "ModLoader DoL ImageLoaderHook",
      "addonName": "ImageLoaderAddon",
      "modVersion": "^2.3.0",
      "params": []
    }
  ],
  "dependenceInfo": [
    {
      "modName": "ModLoader",
      "version": ">=1.1.0"
    },
    {
      "modName": "ModLoader DoL ImageLoaderHook",
      "version": ">=1.0.0"
    },
    {
      "modName": "TweeReplacer",
      "version": ">=1.0.0"
    },
    {
      "modName": "ReplacePatcher",
      "version": ">=1.0.0"
    }
  ]
}