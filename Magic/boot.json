      "modName": "ModLoader",
      "version": ">=1.1.0"
    },
    {
      "modName": "ModLoader DoL ImageLoaderHook",
      "version": ">=1.0.0"
    },
    {
      "modName": "TweeReplacer",
      "version": ">=1.0.0"
    },
    {
      "modName": "ReplacePatcher",
      "version": ">=1.0.0"
    }
  ]
}